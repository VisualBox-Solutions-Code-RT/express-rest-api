{
    "name": "express-rest-api",
    "version": "1.0.0",
    "description": "Node.js / Express.js 4.17.1 - REST API, JWT, TFA, MSSQL, SendGrid, Azure Storage and Docker ready",
    "repository": {
        "url": "https://github.com/shammelburg/express-rest-api"
    },
    "main": "app.js",
    "engines": {
        "node": "12.19.1",
        "npm": "6.14.9"
    },
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon -r dotenv/config app.js dotenv_config_path=.env",
        "lint": "eslint . --ext .js",
        "seed:sqldb": "node data/sql/seed/seed-objects.js dotenv_config_path=.env",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Sander Hammelburg",
    "license": "MIT",
    "dependencies": {
        "@azure/storage-blob": "12.2.1",
        "body-parser": "1.19.0",
        "camelcase-keys": "6.2.2",
        "chalk": "4.1.0",
        "compression": "1.7.4",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "express-fileupload": "1.2.0",
        "helmet": "4.1.1",
        "jsonwebtoken": "8.5.1",
        "morgan": "1.10.0",
        "mssql": "6.2.3",
        "nodemailer": "6.4.13",
        "nodemailer-express-handlebars": "4.0.0",
        "qrcode": "1.4.4",
        "speakeasy": "2.0.0",
        "uuid": "8.3.1",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "eslint": "7.10.0",
        "nodemon": "2.0.4"
    }
}